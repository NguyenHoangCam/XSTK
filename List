def sap_xep_ds_tang (list):
    for me in range (len (list)):
        tạm thời = tôi
        for j in range (i + 1, len (list)):
            if list [j] <list [temp]:
                temp = j
        list [i], list [temp] = list [temp], list [i]
print ("Nhap vao danh sach:")

danhSach = input (). tách ()

if len (danhSach) == 0:
    print ("Danh sach rong")
khác:
    thử:
        list = list (map (int, danhSach))
